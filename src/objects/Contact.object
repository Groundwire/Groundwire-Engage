    <fields>
        <fullName>Engagement_Level_Override__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>This will increase the contact&apos;s engagement level, if this level is higher than what they would otherwise get based on their activity/donor history.  It cannot lower their level, only raise it.  It is effective until the override end date specified.</inlineHelpText>
        <label>Engagement Level Override</label>
        <picklist>
            <picklistValues>
                <fullName>1 - Observing</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>2 - Following</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>3 - Endorsing</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>4 - Contributing</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>5 - Owning</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>6 - Leading</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>Picklist</type>
    </fields>
