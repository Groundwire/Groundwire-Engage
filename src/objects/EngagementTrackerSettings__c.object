    <fields>
        <fullName>Batch_Size__c</fullName>
        <defaultValue>50</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>The number of records processed per batch. Defaults to 50. The default should only be lowered if custom code is causing malfunctioning in the batch processing.</inlineHelpText>
        <label>Batch Size</label>
        <precision>3</precision>
        <required>true</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Householded_Donor_Levels__c</fullName>
        <defaultValue>true</defaultValue>
        <externalId>false</externalId>
