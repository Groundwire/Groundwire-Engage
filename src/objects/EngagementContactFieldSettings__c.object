<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Field_Type__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The datatype of the Contact field.  Only &quot;checkbox&quot; or &quot;text&quot; is supported.  &quot;text&quot; handles picklists, email, address, etc.</inlineHelpText>
        <label>Field Type</label>
        <length>50</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Field_Value__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The value that the field must have in order to qualify for the level.  Enter an asterisk (*) to match any non-empty value. For checkboxes, leave this blank.</inlineHelpText>
        <label>Field Value</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Fieldname__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The Contact field.  Only fields on the Contact object (not Account etc.) can be used here.</inlineHelpText>
        <label>Fieldname</label>
        <length>150</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Level_Value__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The Engagement Level earned by this contact if they have the field value specified.  For checkboxes, the level is earned if it&apos;s checked.</inlineHelpText>
        <label>Engagement Level</label>
        <precision>2</precision>
        <required>true</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>Groundwire Engage Contact Field Settings</label>
</CustomObject>
