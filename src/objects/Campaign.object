<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Engagement_Conversion_Rate__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>What pct. of possible conversions were converted to this engagement level or higher, by this campaign and any sub-campaigns.</inlineHelpText>
        <label>Engagement Conversion Rate</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Engagement_Conversions__c</fullName>
        <externalId>false</externalId>
        <label>Engagement Conversions</label>
        <summaryFilterItems>
            <field>CampaignMember.Engagement_Conversion__c</field>
            <operation>equals</operation>
            <value>True</value>
        </summaryFilterItems>
        <summaryForeignKey>CampaignMember.CampaignId</summaryForeignKey>
        <summaryOperation>count</summaryOperation>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>Engagement_Conversions_in_Hierarchy__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Number of contacts that were moved up an engagement level due to this campaign or one of its sub-campaigns.</inlineHelpText>
        <label>Engagement Conversions in Hierarchy</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Engagement_Level_Numeric__c</fullName>
        <externalId>false</externalId>
        <formula>VALUE(LEFT(TEXT( Leadership_Level__c ),1))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Campaign Engagement Level Numeric</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Engagement_Response_Rate__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>includes sub-campaigns in hierarchy that have same or higher engagement level</inlineHelpText>
        <label>Engagement Response Rate</label>
        <precision>4</precision>
        <required>false</required>
        <scale>1</scale>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Gratitude_Machine__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>True if this campaign for tracking contacts with the Gratitude Machine</inlineHelpText>
        <label>Gratitude Machine</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Leadership_Level__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Level of leadership: 1-organizing, 2-consistent leader, 3-inconsistent leader, 4-activist, 5-donor/member</inlineHelpText>
        <label>Campaign Engagement Level</label>
        <picklist>
            <picklistValues>
                <fullName>1 - Observing</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>2 - Following</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>3 - Endorsing</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>4 - Contributing</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>5 - Owning</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>6 - Leading</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Levels_Below_Top_of_Hierarchy__c</fullName>
        <externalId>false</externalId>
        <formula>IF ( Parent.Parent.Parent.Parent.Id != null , 4 ,
IF (Parent.Parent.Parent.Id != null, 3,
IF (Parent.Parent.Id != null, 2,
IF (Parent.Id != null, 1 , 0)
)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Levels Below Top of Hierarchy</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>New_Engagement_Conversions__c</fullName>
        <externalId>false</externalId>
        <label>New Engagement Conversions</label>
        <summaryFilterItems>
            <field>CampaignMember.Engagement_Conversion__c</field>
            <operation>equals</operation>
            <value>True</value>
        </summaryFilterItems>
        <summaryFilterItems>
            <field>CampaignMember.Engagement_Conversion_Type__c</field>
            <operation>equals</operation>
            <value>New</value>
        </summaryFilterItems>
        <summaryForeignKey>CampaignMember.CampaignId</summaryForeignKey>
        <summaryOperation>count</summaryOperation>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>Number_of_Opp_Engagement_Conversions__c</fullName>
        <externalId>false</externalId>
        <label>Number of Opp Engagement Conversions</label>
        <precision>8</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Response_Conversion_Percentage__c</fullName>
        <externalId>false</externalId>
        <formula>Engagement_Conversions__c /  NumberOfResponses</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Among responses, what percentage of them were engagement conversions (i.e. participation in this campaign meant the participant moved up in engagement level)</inlineHelpText>
        <label>Response Conversion Percentage</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Returning_Engagement_Conversions__c</fullName>
        <externalId>false</externalId>
        <label>Returning Engagement Conversions</label>
        <summaryFilterItems>
            <field>CampaignMember.Engagement_Conversion__c</field>
            <operation>equals</operation>
            <value>True</value>
        </summaryFilterItems>
        <summaryFilterItems>
            <field>CampaignMember.Engagement_Conversion_Type__c</field>
            <operation>equals</operation>
            <value>Returning</value>
        </summaryFilterItems>
        <summaryForeignKey>CampaignMember.CampaignId</summaryForeignKey>
        <summaryOperation>count</summaryOperation>
        <type>Summary</type>
    </fields>
